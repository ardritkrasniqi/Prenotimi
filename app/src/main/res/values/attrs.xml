<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="DayViewArdrit">
        <attr name="eachRowHeight" format="dimension"/>
        <attr name="minimumPercentage" format="float"/>
        <attr name="dividerColor" format="color"/>
        <attr name="dividerTextColor" format="color"/>
        <attr name="dividerTitles" format="color"/>
        <attr name="dividerStartOffset" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CalendarDayView">
        <attr name="dayHeight" format="dimension"/>
        <attr name="debug" format="boolean"/>
        <attr name="eventMarginLeft" format="dimension"/>
        <attr name="lineColor" format="color"/>
        <attr name="textHourSize" format="dimension"/>
        <attr name="startHour" format="integer"/>
        <attr name="endHour" format="integer"/>
    </declare-styleable>
</resources>